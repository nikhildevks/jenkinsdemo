pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    stage('Build') {
            steps {
                echo 'Buildng project'
            }
        }    
    stage('Deploy') {
            steps {
                echo 'Deploying project'
            }
        }    
    stage('Test') {
            steps {
                echo 'Testing project'
            }
        }    
        
    stage('Release') {
            steps {
                echo 'Releasing project'
            }
        }      
        
        
        
        
        
    }
}

